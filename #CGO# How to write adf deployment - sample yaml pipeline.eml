trigger: none



parameters:

  - name: environment

    default: test

    values:

      - test

      - acceptance

      - production



resources:

  repositories:

    - repository: adf-publish

      type: git

      name: rezo-infra-adf

      ref: adf_publish

    - repository: adf-code

      type: git

      name: rezo-infra-adf

      ref: $(Build.SourceBranch)

    - repository: templates

      type: git

      name: GRD0001045/pita-pipeline-templates



variables:

  - name: sourceDataFactoryName

    value: remz02-d-01-df



  - ${{ if eq(parameters.environment, 'test') }}:

    - group: AbnAmro-remz02-adf-test-cicd

    - name: resourceGroup

      value: remz02-t-rg

    - name: managedIdentityName

      value: remz02-t-uami

    - name: databricksWorkspaceName

      value: remz02-t-01-dbr

    - name: ADBRoleName

      value: Contributor

    - name: azureEnvironment

      value: remz02-t-rg-cicd01



  - ${{ if eq(parameters.environment, 'acceptance') }}:

    - group: AbnAmro-remz02-adf-acc-cicd

    - name: resourceGroup

      value: remz02-a-rg

    - name: managedIdentityName

      value: remz02-a-uami

    - name: databricksWorkspaceName

      value: remz02-a-01-dbr

    - name: ADBRoleName

      value: Contributor

    - name: azureEnvironment

      value: remz02-a-rg-cicd01



  - ${{ if eq(parameters.environment, 'production') }}:

    - group: AbnAmro-remz02-adf-prod-cicd

    - name: resourceGroup

      value: remz02-p-rg

    - name: managedIdentityName

      value: remz02-p-uami

    - name: databricksWorkspaceName

      value: remz02-p-01-dbr

    - name: ADBRoleName

      value: Contributor

    - name: azureEnvironment

      value: remz02-p-rg-cicd01



pool:

  vmImage: 'ubuntu-latest'



stages:

- stage: 'remz02_${{ parameters.environment }}_ADFCI'



  jobs:



  # Creating a SNOW ticket if the deplopment is for - acceptance/production

  - ${{ if in(parameters.environment, 'acceptance', 'production') }}:

